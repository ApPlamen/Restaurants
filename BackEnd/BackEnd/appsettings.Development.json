{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Restaurants": "Data Source=.;Initial Catalog=Restaurants;Integrated Security=True"
  },
  "AppSettings": {
    "ConfigurationManagementApiBaseUrl": "http",
    "Environment": "Prod",
    "FrontEndHost": "http://localhost:4200"
  },
  "Jwt": {
    "JwtKey": "SOME_RANDOM_KEY_DO_NOT_SHARE",
    "JwtIssuer": "http://localhost:5000",
    "JwtExpiresIn": 3600,
    "JwtRefreshExpiresIn": 43200,
    "JwtClockSkew": 300
  },
  "AllowedHosts": "*"
}